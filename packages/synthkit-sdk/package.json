{
  "name": "@synthkit/sdk",
  "version": "0.1.0",
  "description": "Core SDK for synthkit - scenario generation, AI analysis, config, and state management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "catalog:",
    "@faker-js/faker": "catalog:",
    "json-schema-faker": "catalog:",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@types/json-schema": "catalog:",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "synthkit",
    "sdk",
    "mock-data",
    "scenario-generation",
    "ai",
    "claude"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholasswanson/synthkit",
    "directory": "packages/synthkit-sdk"
  },
  "license": "MIT"
}
