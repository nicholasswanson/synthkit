{
  "name": "@synthkit/client",
  "version": "0.1.0",
  "description": "React components and providers for Synthkit",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@synthkit/sdk": "workspace:*",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@synthkit/sdk": "workspace:*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "synthkit",
    "react",
    "components",
    "mock-data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholasswanson/synthkit",
    "directory": "packages/synthkit-client"
  },
  "license": "MIT"
}
