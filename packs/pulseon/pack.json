{
  "id": "pulseon",
  "name": "Pulseon Fitness Platform",
  "version": "1.0.0",
  "description": "Fitness streaming platform with workouts, trainers, and health tracking",
  "schemas": {
    "member": {
      "type": "object",
      "description": "Fitness platform member with health metrics",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "email": {
          "type": "string",
          "format": "email",
          "faker": "internet.email"
        },
        "firstName": {
          "type": "string",
          "faker": "person.firstName"
        },
        "lastName": {
          "type": "string",
          "faker": "person.lastName"
        },
        "age": {
          "type": "integer",
          "minimum": 16,
          "maximum": 80,
          "faker": "number.int"
        },
        "fitnessLevel": {
          "type": "string",
          "enum": ["beginner", "intermediate", "advanced", "expert"],
          "faker": "helpers.arrayElement"
        },
        "goals": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["weight_loss", "muscle_gain", "endurance", "flexibility", "strength", "general_fitness"]
          }
        },
        "subscriptionTier": {
          "type": "string",
          "enum": ["free", "basic", "premium", "elite"],
          "default": "free"
        },
        "monthlyFee": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "totalSpent": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "workoutsCompleted": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 1000
        },
        "totalWorkoutMinutes": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 50000
        },
        "averageWorkoutsPerWeek": {
          "type": "number",
          "minimum": 0,
          "maximum": 14,
          "faker": "number.float",
          "precision": 0.1
        },
        "streakDays": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 365
        },
        "preferredWorkoutTypes": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["cardio", "strength", "yoga", "pilates", "hiit", "dance", "meditation", "stretching"]
          }
        },
        "equipment": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["none", "dumbbells", "resistance_bands", "yoga_mat", "kettlebells", "pull_up_bar", "full_gym"]
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.past"
        }
      },
      "required": ["id", "email", "firstName", "lastName", "fitnessLevel", "subscriptionTier", "createdAt"]
    },
    "trainer": {
      "type": "object",
      "description": "Fitness trainer with specializations and metrics",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "email": {
          "type": "string",
          "format": "email",
          "faker": "internet.email"
        },
        "firstName": {
          "type": "string",
          "faker": "person.firstName"
        },
        "lastName": {
          "type": "string",
          "faker": "person.lastName"
        },
        "bio": {
          "type": "string",
          "faker": "lorem.paragraph"
        },
        "specializations": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["cardio", "strength", "yoga", "pilates", "hiit", "dance", "meditation", "stretching", "nutrition"]
          }
        },
        "certifications": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["ACSM", "NASM", "ACE", "NSCA", "RYT-200", "RYT-500", "Precision Nutrition"]
          }
        },
        "yearsExperience": {
          "type": "integer",
          "minimum": 0,
          "maximum": 30,
          "faker": "number.int"
        },
        "workoutsCreated": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 500
        },
        "totalFollowers": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 1000000
        },
        "averageRating": {
          "type": "number",
          "minimum": 1,
          "maximum": 5,
          "faker": "number.float",
          "precision": 0.1
        },
        "totalEarnings": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "monthlyEarnings": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "isVerified": {
          "type": "boolean",
          "default": false
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.past"
        }
      },
      "required": ["id", "email", "firstName", "lastName", "specializations", "createdAt"]
    },
    "workout": {
      "type": "object",
      "description": "Fitness workout with exercises and metrics",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "trainerId": {
          "type": "string",
          "format": "uuid"
        },
        "title": {
          "type": "string",
          "faker": "lorem.sentence"
        },
        "description": {
          "type": "string",
          "faker": "lorem.paragraph"
        },
        "type": {
          "type": "string",
          "enum": ["cardio", "strength", "yoga", "pilates", "hiit", "dance", "meditation", "stretching"],
          "faker": "helpers.arrayElement"
        },
        "level": {
          "type": "string",
          "enum": ["beginner", "intermediate", "advanced"],
          "faker": "helpers.arrayElement"
        },
        "duration": {
          "type": "integer",
          "minimum": 5,
          "maximum": 120,
          "description": "Duration in minutes"
        },
        "equipment": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["none", "dumbbells", "resistance_bands", "yoga_mat", "kettlebells", "pull_up_bar"]
          }
        },
        "caloriesBurned": {
          "type": "integer",
          "minimum": 50,
          "maximum": 800,
          "faker": "number.int"
        },
        "completions": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 100000
        },
        "averageRating": {
          "type": "number",
          "minimum": 1,
          "maximum": 5,
          "faker": "number.float",
          "precision": 0.1
        },
        "reviewCount": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 5000
        },
        "subscriptionTier": {
          "type": "string",
          "enum": ["free", "basic", "premium", "elite"],
          "default": "free"
        },
        "isPublished": {
          "type": "boolean",
          "default": true
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.past"
        }
      },
      "required": ["id", "trainerId", "title", "type", "level", "duration", "createdAt"]
    },
    "session": {
      "type": "object",
      "description": "Member workout session with performance data",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "memberId": {
          "type": "string",
          "format": "uuid"
        },
        "workoutId": {
          "type": "string",
          "format": "uuid"
        },
        "status": {
          "type": "string",
          "enum": ["started", "paused", "completed", "abandoned"],
          "default": "completed"
        },
        "duration": {
          "type": "integer",
          "minimum": 0,
          "description": "Actual duration in minutes"
        },
        "caloriesBurned": {
          "type": "integer",
          "minimum": 0,
          "faker": "number.int",
          "max": 800
        },
        "averageHeartRate": {
          "type": "integer",
          "minimum": 60,
          "maximum": 200,
          "faker": "number.int"
        },
        "maxHeartRate": {
          "type": "integer",
          "minimum": 80,
          "maximum": 220,
          "faker": "number.int"
        },
        "completionPercentage": {
          "type": "number",
          "minimum": 0,
          "maximum": 1,
          "faker": "number.float",
          "precision": 0.01
        },
        "rating": {
          "type": "integer",
          "minimum": 1,
          "maximum": 5,
          "faker": "number.int"
        },
        "notes": {
          "type": "string",
          "faker": "lorem.sentence"
        },
        "startedAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.recent"
        },
        "completedAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.recent"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.recent"
        }
      },
      "required": ["id", "memberId", "workoutId", "status", "duration", "startedAt", "createdAt"]
    }
  },
  "categories": {
    "early": {
      "id": "early",
      "name": "Boutique Fitness Studio",
      "description": "Small fitness streaming platform with core offerings",
      "businessType": "fitness",
      "config": {
        "id": 10003,
        "dateRange": {
          "start": "2024-04-01",
          "end": "2024-12-31"
        },
        "volume": {
          "members": 800,
          "trainers": 15,
          "workouts": 200,
          "sessions": 5000
        },
        "relationships": {
          "averageSubscriptionFee": 29,
          "completionRate": 0.75,
          "retentionRate": 0.68,
          "trainerRevShare": 0.60
        },
        "complexity": "early"
      }
    },
    "growth": {
      "id": "growth",
      "name": "Growing Fitness Platform",
      "description": "Expanding fitness platform with diverse content",
      "businessType": "fitness",
      "config": {
        "id": 20003,
        "dateRange": {
          "start": "2023-01-01",
          "end": "2024-12-31"
        },
        "volume": {
          "members": 25000,
          "trainers": 200,
          "workouts": 2500,
          "sessions": 200000
        },
        "relationships": {
          "averageSubscriptionFee": 49,
          "completionRate": 0.82,
          "retentionRate": 0.75,
          "trainerRevShare": 0.55
        },
        "complexity": "growth"
      }
    },
    "enterprise": {
      "id": "enterprise",
      "name": "Global Fitness Network",
      "description": "Large-scale fitness platform serving millions",
      "businessType": "fitness",
      "config": {
        "id": 30003,
        "dateRange": {
          "start": "2022-01-01",
          "end": "2024-12-31"
        },
        "volume": {
          "members": 1000000,
          "trainers": 5000,
          "workouts": 50000,
          "sessions": 20000000
        },
        "relationships": {
          "averageSubscriptionFee": 79,
          "completionRate": 0.88,
          "retentionRate": 0.82,
          "trainerRevShare": 0.50
        },
        "complexity": "enterprise"
      }
    }
  },
  "roles": {
    "admin": {
      "id": "admin",
      "name": "Administrator",
      "description": "Full access to all fitness and financial data",
      "accessLevel": "admin",
      "dataVisibility": {
        "fullAccess": true
      },
      "overrides": {
        "trainer": {
          "isVerified": true
        },
        "member": {
          "subscriptionTier": "premium"
        }
      }
    },
    "support": {
      "id": "support",
      "name": "Support Agent",
      "description": "Member support with financial data masked",
      "accessLevel": "support",
      "dataVisibility": {
        "fullAccess": false,
        "maskedFields": ["monthlyFee", "totalSpent", "totalEarnings", "monthlyEarnings"],
        "hiddenFields": []
      },
      "overrides": {}
    }
  },
  "routes": {
    "/api/members": {
      "schema": "member",
      "rest": true,
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "/api/trainers": {
      "schema": "trainer",
      "rest": true,
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "/api/workouts": {
      "schema": "workout",
      "rest": true,
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "/api/sessions": {
      "schema": "session",
      "rest": true,
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  }
}
