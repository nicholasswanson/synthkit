{
  "id": "keynest",
  "name": "Keynest Management Platform",
  "version": "1.0.0",
  "description": "Real estate property management with tenants, leases, and maintenance tracking",
  "schemas": {
    "property": {
      "type": "object",
      "description": "Real estate property with details and financials",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "address": { "type": "object", "properties": { "street": { "type": "string", "faker": "location.streetAddress" }, "city": { "type": "string", "faker": "location.city" }, "state": { "type": "string", "faker": "location.state" }, "zipCode": { "type": "string", "faker": "location.zipCode" } } },
        "type": { "type": "string", "enum": ["apartment", "house", "condo", "townhouse", "commercial", "office"], "faker": "helpers.arrayElement" },
        "bedrooms": { "type": "integer", "minimum": 0, "maximum": 10, "faker": "number.int" },
        "bathrooms": { "type": "number", "minimum": 0, "maximum": 10, "faker": "number.float", "precision": 0.5 },
        "squareFeet": { "type": "integer", "minimum": 300, "maximum": 10000, "faker": "number.int" },
        "yearBuilt": { "type": "integer", "minimum": 1900, "maximum": 2024, "faker": "number.int" },
        "purchasePrice": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "currentValue": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "monthlyRent": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "monthlyExpenses": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "occupancyRate": { "type": "number", "minimum": 0, "maximum": 1, "faker": "number.float", "precision": 0.01 },
        "amenities": { "type": "array", "items": { "type": "string", "enum": ["parking", "pool", "gym", "laundry", "balcony", "ac", "heating", "dishwasher"] } },
        "status": { "type": "string", "enum": ["available", "occupied", "maintenance", "renovation"], "default": "occupied" },
        "createdAt": { "type": "string", "format": "date-time", "faker": "date.past" }
      },
      "required": ["id", "address", "type", "squareFeet", "monthlyRent", "status", "createdAt"]
    },
    "tenant": {
      "type": "object",
      "description": "Property tenant with lease information",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "firstName": { "type": "string", "faker": "person.firstName" },
        "lastName": { "type": "string", "faker": "person.lastName" },
        "email": { "type": "string", "format": "email", "faker": "internet.email" },
        "phone": { "type": "string", "faker": "phone.number" },
        "employmentStatus": { "type": "string", "enum": ["employed", "self_employed", "unemployed", "student", "retired"], "faker": "helpers.arrayElement" },
        "monthlyIncome": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "creditScore": { "type": "integer", "minimum": 300, "maximum": 850, "faker": "number.int" },
        "emergencyContact": { "type": "object", "properties": { "name": { "type": "string", "faker": "person.fullName" }, "phone": { "type": "string", "faker": "phone.number" }, "relationship": { "type": "string", "faker": "lorem.word" } } },
        "totalRentPaid": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "latePayments": { "type": "integer", "minimum": 0, "faker": "number.int", "max": 12 },
        "moveInDate": { "type": "string", "format": "date-time", "faker": "date.past" },
        "leaseEndDate": { "type": "string", "format": "date-time", "faker": "date.future" },
        "status": { "type": "string", "enum": ["active", "notice_given", "moved_out", "evicted"], "default": "active" },
        "createdAt": { "type": "string", "format": "date-time", "faker": "date.past" }
      },
      "required": ["id", "firstName", "lastName", "email", "employmentStatus", "moveInDate", "status", "createdAt"]
    },
    "lease": {
      "type": "object",
      "description": "Property lease agreement with terms",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "propertyId": { "type": "string", "format": "uuid" },
        "tenantId": { "type": "string", "format": "uuid" },
        "startDate": { "type": "string", "format": "date-time", "faker": "date.past" },
        "endDate": { "type": "string", "format": "date-time", "faker": "date.future" },
        "monthlyRent": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "securityDeposit": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "petDeposit": { "type": "number", "minimum": 0, "default": 0 },
        "leaseTerm": { "type": "integer", "minimum": 1, "maximum": 24, "description": "Lease term in months" },
        "renewalOption": { "type": "boolean", "default": true },
        "petsAllowed": { "type": "boolean", "default": false },
        "smokingAllowed": { "type": "boolean", "default": false },
        "utilities": { "type": "object", "properties": { "water": { "type": "string", "enum": ["tenant", "landlord", "shared"] }, "electric": { "type": "string", "enum": ["tenant", "landlord", "shared"] }, "gas": { "type": "string", "enum": ["tenant", "landlord", "shared"] }, "internet": { "type": "string", "enum": ["tenant", "landlord", "shared"] } } },
        "status": { "type": "string", "enum": ["active", "expired", "terminated", "renewed"], "default": "active" },
        "createdAt": { "type": "string", "format": "date-time", "faker": "date.past" }
      },
      "required": ["id", "propertyId", "tenantId", "startDate", "endDate", "monthlyRent", "leaseTerm", "status", "createdAt"]
    },
    "payment": {
      "type": "object",
      "description": "Rent payment transaction",
      "properties": {
        "id": { "type": "string", "format": "uuid" },
        "leaseId": { "type": "string", "format": "uuid" },
        "tenantId": { "type": "string", "format": "uuid" },
        "amount": { "type": "number", "minimum": 0, "faker": "commerce.price" },
        "paymentType": { "type": "string", "enum": ["rent", "late_fee", "security_deposit", "pet_deposit", "utilities", "maintenance"], "default": "rent" },
        "paymentMethod": { "type": "string", "enum": ["ach", "credit_card", "check", "cash", "money_order"], "faker": "helpers.arrayElement" },
        "dueDate": { "type": "string", "format": "date-time", "faker": "date.recent" },
        "paidDate": { "type": "string", "format": "date-time", "faker": "date.recent" },
        "lateFee": { "type": "number", "minimum": 0, "default": 0 },
        "status": { "type": "string", "enum": ["pending", "paid", "late", "failed", "refunded"], "default": "paid" },
        "createdAt": { "type": "string", "format": "date-time", "faker": "date.past" }
      },
      "required": ["id", "leaseId", "tenantId", "amount", "paymentType", "dueDate", "status", "createdAt"]
    }
  },
  "categories": {
    "early": {
      "id": "early",
      "name": "Small Property Portfolio",
      "description": "Individual landlord with small property portfolio",
      "businessType": "real_estate",
      "config": {
        "id": 10006,
        "dateRange": { "start": "2024-01-01", "end": "2024-12-31" },
        "volume": { "properties": 25, "tenants": 30, "leases": 35, "payments": 400 },
        "relationships": { "averageRent": 1850, "occupancyRate": 0.92, "onTimePayment": 0.88, "maintenanceCosts": 0.08 },
        "complexity": "early"
      }
    },
    "growth": {
      "id": "growth",
      "name": "Regional Property Management",
      "description": "Growing property management company with diverse portfolio",
      "businessType": "real_estate",
      "config": {
        "id": 20006,
        "dateRange": { "start": "2023-01-01", "end": "2024-12-31" },
        "volume": { "properties": 500, "tenants": 650, "leases": 750, "payments": 9000 },
        "relationships": { "averageRent": 2200, "occupancyRate": 0.95, "onTimePayment": 0.92, "maintenanceCosts": 0.06 },
        "complexity": "growth"
      }
    },
    "enterprise": {
      "id": "enterprise",
      "name": "National Property Management",
      "description": "Large property management company with multi-market presence",
      "businessType": "real_estate",
      "config": {
        "id": 30006,
        "dateRange": { "start": "2022-01-01", "end": "2024-12-31" },
        "volume": { "properties": 10000, "tenants": 12500, "leases": 15000, "payments": 180000 },
        "relationships": { "averageRent": 2650, "occupancyRate": 0.97, "onTimePayment": 0.95, "maintenanceCosts": 0.04 },
        "complexity": "enterprise"
      }
    }
  },
  "roles": {
    "admin": {
      "id": "admin",
      "name": "Administrator",
      "description": "Full access to all property and financial data",
      "accessLevel": "admin",
      "dataVisibility": { "fullAccess": true },
      "overrides": { "property": { "status": "occupied" }, "tenant": { "status": "active" } }
    },
    "support": {
      "id": "support",
      "name": "Support Agent",
      "description": "Tenant support with financial data masked",
      "accessLevel": "support",
      "dataVisibility": { "fullAccess": false, "maskedFields": ["purchasePrice", "currentValue", "monthlyRent", "monthlyExpenses", "monthlyIncome", "totalRentPaid", "securityDeposit", "petDeposit", "amount", "lateFee"], "hiddenFields": [] },
      "overrides": {}
    }
  },
  "routes": {
    "/api/properties": { "schema": "property", "rest": true, "methods": ["GET", "POST", "PUT", "DELETE"] },
    "/api/tenants": { "schema": "tenant", "rest": true, "methods": ["GET", "POST", "PUT", "DELETE"] },
    "/api/leases": { "schema": "lease", "rest": true, "methods": ["GET", "POST", "PUT", "DELETE"] },
    "/api/payments": { "schema": "payment", "rest": true, "methods": ["GET", "POST", "PUT"] }
  }
}
