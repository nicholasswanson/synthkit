{
  "id": "stratus",
  "name": "Stratus SaaS",
  "version": "1.0.0",
  "description": "B2B SaaS platform with subscriptions, billing, and usage analytics",
  "schemas": {
    "customer": {
      "type": "object",
      "description": "SaaS customer account",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "companyName": {
          "type": "string",
          "faker": "company.name"
        },
        "email": {
          "type": "string",
          "format": "email",
          "faker": "internet.email"
        },
        "industry": {
          "type": "string",
          "enum": ["technology", "healthcare", "finance", "education", "retail", "manufacturing"],
          "faker": "helpers.arrayElement"
        },
        "employees": {
          "type": "integer",
          "minimum": 1,
          "faker": "number.int",
          "max": 10000
        },
        "monthlyRecurringRevenue": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "lifetimeValue": {
          "type": "number",
          "minimum": 0
        },
        "churnRisk": {
          "type": "string",
          "enum": ["low", "medium", "high"],
          "default": "low"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.past"
        }
      },
      "required": ["id", "companyName", "email", "industry", "createdAt"]
    },
    "subscription": {
      "type": "object",
      "description": "SaaS subscription with billing details",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "customerId": {
          "type": "string",
          "format": "uuid"
        },
        "planId": {
          "type": "string",
          "enum": ["starter", "professional", "enterprise", "custom"]
        },
        "status": {
          "type": "string",
          "enum": ["trialing", "active", "past_due", "canceled", "unpaid"],
          "default": "active"
        },
        "monthlyAmount": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "annualAmount": {
          "type": "number",
          "minimum": 0
        },
        "billingCycle": {
          "type": "string",
          "enum": ["monthly", "annual"],
          "default": "monthly"
        },
        "seats": {
          "type": "integer",
          "minimum": 1,
          "faker": "number.int",
          "max": 1000
        },
        "usageLimit": {
          "type": "integer",
          "minimum": 0
        },
        "currentUsage": {
          "type": "integer",
          "minimum": 0
        },
        "trialEndsAt": {
          "type": "string",
          "format": "date-time"
        },
        "nextBillingAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.future"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.past"
        }
      },
      "required": ["id", "customerId", "planId", "status", "monthlyAmount", "createdAt"]
    },
    "invoice": {
      "type": "object",
      "description": "SaaS billing invoice",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "subscriptionId": {
          "type": "string",
          "format": "uuid"
        },
        "customerId": {
          "type": "string",
          "format": "uuid"
        },
        "invoiceNumber": {
          "type": "string",
          "faker": "finance.accountNumber"
        },
        "amount": {
          "type": "number",
          "minimum": 0,
          "faker": "commerce.price"
        },
        "tax": {
          "type": "number",
          "minimum": 0
        },
        "total": {
          "type": "number",
          "minimum": 0
        },
        "currency": {
          "type": "string",
          "enum": ["USD", "EUR", "GBP"],
          "default": "USD"
        },
        "status": {
          "type": "string",
          "enum": ["draft", "sent", "paid", "overdue", "void"],
          "default": "paid"
        },
        "dueDate": {
          "type": "string",
          "format": "date-time",
          "faker": "date.future"
        },
        "paidAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.recent"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.past"
        }
      },
      "required": ["id", "subscriptionId", "customerId", "invoiceNumber", "amount", "total", "status", "createdAt"]
    },
    "usage": {
      "type": "object",
      "description": "SaaS usage metrics and billing",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "customerId": {
          "type": "string",
          "format": "uuid"
        },
        "subscriptionId": {
          "type": "string",
          "format": "uuid"
        },
        "metric": {
          "type": "string",
          "enum": ["api_calls", "storage_gb", "users", "projects", "bandwidth_gb"],
          "faker": "helpers.arrayElement"
        },
        "quantity": {
          "type": "number",
          "minimum": 0,
          "faker": "number.int",
          "max": 100000
        },
        "unitPrice": {
          "type": "number",
          "minimum": 0
        },
        "totalCost": {
          "type": "number",
          "minimum": 0
        },
        "billingPeriod": {
          "type": "string",
          "format": "date"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "faker": "date.recent"
        }
      },
      "required": ["id", "customerId", "subscriptionId", "metric", "quantity", "createdAt"]
    }
  },
  "categories": {
    "early": {
      "id": "early",
      "name": "Early Stage Startup",
      "description": "SaaS startup with initial customer traction",
      "businessType": "saas",
      "config": {
        "id": 44444,
        "dateRange": {
          "start": "2024-03-01",
          "end": "2024-12-31"
        },
        "volume": {
          "customers": 150,
          "subscriptions": 180,
          "invoices": 800,
          "usage": 5000
        },
        "relationships": {
          "trialConversionRate": 0.25,
          "churnRate": 0.12,
          "averageRevenuePerUser": 89,
          "growthRate": 0.15
        },
        "complexity": "early"
      }
    },
    "growth": {
      "id": "growth",
      "name": "Scaling SaaS Company",
      "description": "Growing SaaS with established market fit",
      "businessType": "saas",
      "config": {
        "id": 55555,
        "dateRange": {
          "start": "2023-01-01",
          "end": "2024-12-31"
        },
        "volume": {
          "customers": 2500,
          "subscriptions": 3200,
          "invoices": 15000,
          "usage": 100000
        },
        "relationships": {
          "trialConversionRate": 0.35,
          "churnRate": 0.08,
          "averageRevenuePerUser": 245,
          "growthRate": 0.25
        },
        "complexity": "growth"
      }
    },
    "enterprise": {
      "id": "enterprise",
      "name": "Enterprise SaaS Platform",
      "description": "Large-scale SaaS serving enterprise customers",
      "businessType": "saas",
      "config": {
        "id": 66666,
        "dateRange": {
          "start": "2022-01-01",
          "end": "2024-12-31"
        },
        "volume": {
          "customers": 15000,
          "subscriptions": 18500,
          "invoices": 75000,
          "usage": 2000000
        },
        "relationships": {
          "trialConversionRate": 0.45,
          "churnRate": 0.04,
          "averageRevenuePerUser": 850,
          "growthRate": 0.35
        },
        "complexity": "enterprise"
      }
    }
  },
  "roles": {
    "admin": {
      "id": "admin",
      "name": "Administrator",
      "description": "Full access to all revenue and customer data",
      "accessLevel": "admin",
      "dataVisibility": {
        "fullAccess": true
      },
      "overrides": {
        "subscription": {
          "planId": "enterprise",
          "billingCycle": "annual"
        },
        "customer": {
          "churnRisk": "low"
        }
      }
    },
    "support": {
      "id": "support",
      "name": "Support Agent",
      "description": "Customer support with financial data masked",
      "accessLevel": "support",
      "dataVisibility": {
        "fullAccess": false,
        "maskedFields": ["monthlyRecurringRevenue", "lifetimeValue", "monthlyAmount", "annualAmount", "amount", "tax", "total", "unitPrice", "totalCost"],
        "hiddenFields": []
      },
      "overrides": {}
    }
  },
  "routes": {
    "/api/customers": {
      "schema": "customer",
      "rest": true,
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "/api/subscriptions": {
      "schema": "subscription",
      "rest": true,
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    "/api/invoices": {
      "schema": "invoice",
      "rest": true,
      "methods": ["GET", "POST", "PUT"]
    },
    "/api/usage": {
      "schema": "usage",
      "rest": true,
      "methods": ["GET", "POST"]
    }
  }
}
